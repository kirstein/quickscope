{
  "name": "quickscope",
  "version": "0.3.1",
  "description": "Quickly react to dependecy changes",
  "scripts": {
    "test": "jest",
    "watch": "quickscope",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kirstein/quickscope.git"
  },
  "main": "./quickscope",
  "devDependencies": {
    "@testlio/eslint-config-services": "^1.1.0",
    "eslint": "^3.5.0",
    "jest-cli": "^15.1.1"
  },
  "license": "MIT",
  "dependencies": {
    "chokidar": "^1.2.0",
    "cli-spinner": "^0.2.1",
    "debug": "^2.2.0",
    "dependency-tree": "^5.6.1",
    "find-project-root": "^1.1.1",
    "keymirror": "^0.1.1",
    "lodash": "^4.5.1",
    "minimist": "^1.2.0"
  },
  "config": {
    "quickscope": {
      "files": [
        "(stores|models)/__tests__/**/**.js",
        "__tests__/**/**.js"
      ],
      "cmd": "node node_modules/jest-cli/bin/jest.js"
    }
  }
}
